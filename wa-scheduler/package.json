{
  "name": "wa-scheduler",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=\"--import ./scripts/register-loader.mjs\" next dev",
    "build": "cross-env NODE_OPTIONS=\"--import ./scripts/register-loader.mjs\" next build",
    "build:server": "tsc -p tsconfig.server.json",
    "start": "cross-env NODE_OPTIONS=\"--import ./scripts/register-loader.mjs\" next start",
    "bot": "ts-node -P tsconfig.server.json server/startBot.ts",
    "bot:old": "node dist/baileysClient.js",
    "scheduler": "node dist/scheduler.js",
    "api": "node dist/api.js",
    "test:send": "ts-node -P tsconfig.server.json server/testSend.ts",
    "test:scheduler": "ts-node -P tsconfig.server.json server/testScheduler.ts",
    "test:api": "ts-node -P tsconfig.server.json server/testAPI.ts",
    "health": "ts-node -P tsconfig.server.json server/healthCheck.ts",
    "lint": "cross-env NODE_OPTIONS=\"--import ./scripts/register-loader.mjs\" next lint",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@prisma/client": "^5.22.0",
    "@types/qrcode": "^1.5.6",
    "@whiskeysockets/baileys": "^6.7.21",
    "axios": "^1.13.2",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "next": "14.2.0",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.0",
    "prisma": "^5.22.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.0"
  }
}
